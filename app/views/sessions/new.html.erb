<p id="alert"><%= alert %></p>


<h1>Login Page</h1>



<%= form_tag sessions_path do |form| %>
  <div class="field">
    <%= label_tag :email %><br>
    <%= text_field_tag :email %><br>
  </div>  <div class="field">
    <%= label_tag :password %><br>
    <%= password_field_tag :password %><br>
  </div>  <br>
		<div class="actions">
    <%= submit_tag "Login" %>
  </div>
<% end %>